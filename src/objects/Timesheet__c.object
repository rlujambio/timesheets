    <validationRules>
        <fullName>Check_for_Details_if_Type_is_Other</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Type__c , &apos;Other&apos;), OR(ISBLANK( Type_Details__c ), ISNULL(Type_Details__c)  ))</errorConditionFormula>
